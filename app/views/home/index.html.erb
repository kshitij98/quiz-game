<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% @genres.each do |genre| %>
	<div class="genre">
		<%= genre.name %>
	</div>
	<% arr = @subgenres.select{ |subgenre| subgenre.genre_id.to_i == genre.id.to_i } %>
		<% arr.each do |subgenre| %>
			<div class="subgenre">
				<%= subgenre.name %>
			</div>
		<% end %>
<% end %>
